<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IService1" />
                <binding name="BasicHttpBinding_IService" />
                <binding name="BasicHttpBinding_IService2" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:13159/Service1.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IService1" contract="TemperatureConversionService.IService1"
                name="BasicHttpBinding_IService1" />
            <endpoint address="http://neptune.fulton.ad.asu.edu/WSRepository/Services/EncryptionWcf/Service.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IService"
                contract="EncryptionServiceReference.IService" name="BasicHttpBinding_IService" />
            <endpoint address="http://neptune.fulton.ad.asu.edu/WSRepository/Services/Stockquote/Service.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IService2"
                contract="StockQuoteService.IService" name="BasicHttpBinding_IService2" />
        </client>
    </system.serviceModel>
</configuration>